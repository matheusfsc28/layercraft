{
    "name": "matheusfsc28/layercraft",
    "description": "A package to generate layered architecture (Controller, Service, Interface, Repository, Model) in Laravel applications.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Felipe",
            "email": "dev.matheusfelipe@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Matheusfsc28\\LayerCraft\\App\\": "src/app/"
        },
        "files": [
            "src/app/Helpers/ColorDump.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matheusfsc28\\LayerCraft\\App\\Providers\\LayerCraftServiceProvider"
            ]
        }
    }
}